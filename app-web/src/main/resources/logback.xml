<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<include resource="org/springframework/boot/logging/logback/base.xml" />

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-5p: %c - %m%n</pattern>
		</encoder>
	</appender>

	<!-- logger name="org.springframework" level="DEBUG"/ -->

	<logger name="com.blank.aop" level="FINE" />
	<logger name="com.blank.web.GeneralController" level="FINE" />

	<logger name="org.springframework.webflow" level="FINE" />
	<logger name="com.blank.config.WebFlowConfig" level="FINE" />
	<logger name="com.blank.service" level="FINE" />

</configuration>
